{
  "cli": {
    "version": ">= 7.5.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "env": {
          "ENTRY_FILE": "./src/App.tsx"
        }
      },
      "ios": {
        "env": {
          "ENTRY_FILE": "./applications/mobile/src/App.tsx"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "env": {
          "ENTRY_FILE": "./src/App.tsx"
        }
      },
      "ios": {
        "env": {
          "ENTRY_FILE": "./applications/mobile/src/App.tsx"
        }
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "env": {
          "ENTRY_FILE": "./src/App.tsx"
        }
      },
      "ios": {
        "env": {
          "ENTRY_FILE": "./applications/mobile/src/App.tsx"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
